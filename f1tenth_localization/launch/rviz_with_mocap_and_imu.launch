<launch>
	<param name="/use_sim_time" value="false"/>
	
	<include file="$(find qualisys)/launch/qualisys.launch" />
	
	<param name="mocap_name" value="F1Tenth"/>
	<node pkg="f1tenth_localization" type="mocap_to_odom.py" name="mocap_to_odom"/>
	
	<node pkg="f1tenth_localization" type="set_timestamps.py" name="timestamps"/>
	
	<node pkg="f1tenth_localization" type="imu_to_odom.py" name="imu_to_odom"/>

    <node pkg="tf" type="static_transform_publisher" name="Imu_frame" args="0 0 0 -3.14159 0 0 qualisys base_imu_link 100"/>
	
	<node pkg="rviz" type="rviz" name="rviz" args="-d $(find f1tenth_localization)/rviz/imu_and_mocap.rviz"/>
	
</launch>